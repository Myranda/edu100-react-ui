  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "mock": "NODE_ENV=mock webpack-dev-server --inline --progress --config build/webpack.mock.conf.js",
    "start": "yarn run dev",
    "analyz": "NODE_ENV=production npm_config_report=true yarn run build:production",
    "unit": "bash support/unit-test.sh",
    "lint": "bash support/eslint.sh",
    "format": "pretty-quick --staged",
    "precommit": "yarn run format",
    "ssr:production": "node ssr/server.js --NODE_ENV=production",
    "ssr:pre": "node ssr/server.js --NODE_ENV=pre",
    "ssr:testing": "node ssr/server.js --NODE_ENV=testing",
    "component": " webpack --config build/webpack.component.js --display-modules"
  },
  "reactSnap": {
    "source": "dist/testing",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
